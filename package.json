{
  "name": "ember-lookit-frameplayer",
  "version": "0.0.0",
  "description": "Ember Frame Player",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "docs": "yuidoc --lint && yuidoc && mkdir -p themes/lookit/assets/screenshots && rm -f themes/lookit/assets/screenshots/* && cp screenshots/* themes/lookit/assets/screenshots/ && mkdir -p docs/assets/screenshots && rm -f docs/assets/screenshots/* && cp screenshots/* docs/assets/screenshots/",
    "build": "ember build",
    "start": "ember server",
    "test": "yarn run check-style && ember test",
    "check-style": "./node_modules/jscs/bin/jscs .",
    "bump-version": "npm --no-git-tag-version version ",
    "preversion": "git-branch-is develop && yarn test",
    "postversion": "yarn run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lookit/ember-lookit-frameplayer.git"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "ember-bootstrap": "^2.5.0",
    "bootstrap-sass": "^3.4.1",
    "broccoli-auto-generated": "0.0.3",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "dotenv": "^2.0.0",
    "ember-cp-validations": "4.0.0-beta.3",
    "ember-data-url-templates": "^0.1.0",
    "ember-get-config": "0.0.2",
    "ember-js-pdf": "^0.4.2",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "font-awesome": "~4.5.0",
    "git-branch-is": "^2.1.0",
    "lodash": "~4.6.1",
    "node-sass": "^4.11.0",
    "request-promise": "^2.0.1",
    "typescript": "3.2.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^4.0.0",
    "ember-bootstrap-datetimepicker": "1.1.0",
    "ember-cli": "3.5.1",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-dynamic-forms": "0.4.0",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-favicon": "https://github.com/davewasmer/ember-cli-favicon.git#08c6392aa36fb2820a0cf393bd11fb521cbec2e2",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-jwt-decode": "0.0.2",
    "ember-cli-moment-shim": "3.7.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sass": "8.0.1",
    "ember-cli-sentry": "2.4.2",
    "ember-cli-shims": "1.2.0",
    "ember-cli-showdown": "4.4.4",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-template-lint": "^0.7.6",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-collapsible-panel": "0.2.2",
    "ember-data": "^3.5.1",
    "ember-data-has-many-query": "0.2.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-font-awesome": "3.1.1",
    "ember-i18n": "5.3.0",
    "ember-inflector": "1.9.5",
    "ember-load-initializers": "^2.0.0",
    "ember-moment": "6.0.0",
    "ember-power-select": "2.2.0",
    "ember-qunit": "^4.1.2",
    "ember-radio-buttons": "4.0.1",
    "ember-resolver": "^5.0.1",
    "ember-scroll-to": "^0.6.4",
    "ember-simple-auth": "1.1.0",
    "ember-sinon": "3.0.0",
    "ember-sinon-qunit": "3.3.0",
    "ember-source": "^3.4.8",
    "ember-toastr": "1.7.2",
    "ember-truth-helpers": "1.3.0",
    "ember-try": "~1.1.0",
    "jscs": "^3.0.7",
    "loader.js": "^4.7.0",
    "moment": "^2.22.2",
    "moment-timezone": "0.5.23",
    "toastr": "^2.1.4",
    "yuidocjs": "^0.10.2"
  },
  "resolutions": {
    "node-sass": "^4.10.0",
    "**/ember-bootstrap-datetimepicker/ember-cli-babel": "^6.6",
    "**/ember-bootstrap-datetimepicker/ember-runtime-enumerable-includes-polyfill/ember-cli-babel": "^6.6"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    }
  },
  "jscsConfig": {
    "preset": "airbnb",
    "excludeFiles": [
      "package.json",
      "bower.json",
      "testem.js",
      "ember-cli-build.js",
      "config/environment.js",
      "bower_components",
      "node_modules",
      "blueprints",
      "lib",
      "dist",
      "docs",
      "examples",
      "tmp",
      "vendor",
      "app/locales",
      "tests",
      "themes"
    ],
    "requireSpacesInsideObjectBrackets": false,
    "requireSpacesInsideImportedObjectBraces": false,
    "requireSpacesInAnonymousFunctionExpression": false,
    "requireTrailingComma": false,
    "disallowTrailingComma": false,
    "disallowArrayDestructuringReturn": false,
    "disallowQuotedKeysInObjects": false,
    "requirePaddingNewLinesAfterBlocks": false,
    "validateIndentation": 4,
    "requirePaddingNewLinesBeforeLineComments": false,
    "maximumLineLength": false,
    "disallowSpaceBeforeComma": false
  }
}
